#import <Foundation/Foundation.h>

@interface CommentInfo : NSObject
@property(nonatomic, copy)NSString *id;
@property(nonatomic, assign)float score;        //评分
@property(nonatomic, copy)NSString *content;    //评论内容
@property(nonatomic, copy)NSString *createDate;//评论日期
@property(nonatomic, assign)BOOL isShow;        //是否显示
@property(nonatomic, copy)NSString *ip;         //IP
@property(nonatomic, copy)NSString *memberId;   //评论会员ID
@property(nonatomic, copy)NSString *memberName; //评论会员名称
@property(nonatomic, copy)NSString *productId;  //商品ID
@property(nonatomic, copy)NSString *productName;//商品名称
@property(nonatomic, copy)NSString *productImage;
@property(nonatomic, copy)NSString *productSn;  //商品SN
@end
